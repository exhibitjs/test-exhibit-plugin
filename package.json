{
  "name": "test-exhibit-plugin",
  "description": "Asserts Exhibit plugin fixtures",
  "version": "0.1.2",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "bin": {
    "test-exhibit-plugin": "dist/cli.js"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "chalk": "^1.1.3",
    "figures": "^1.5.0",
    "loud-rejection": "^1.3.0",
    "minimist": "^1.2.0",
    "sander": "^0.4.0",
    "source-map-support": "^0.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "del-cli": "^0.2.0",
    "eslint": "^2.6.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-babel": "^3.1.0"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "peerDependencies": {
    "exhibit": "^0.8.3"
  },
  "repository": "exhibitjs/test-exhibit-plugin",
  "scripts": {
    "build": "del dist && babel src --out-dir dist --source-maps",
    "build:watch": "del dist && babel src --out-dir dist --watch --source-maps",
    "prepublish": "npm run build",
    "test": "eslint ."
  }
}
